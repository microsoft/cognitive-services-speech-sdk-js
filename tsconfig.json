{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
